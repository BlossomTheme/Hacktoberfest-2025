{
  "name": "GitHub Professional Theme",
  "version": "1.0.0",
  "description": "A clean, modern, and professional theme for GitHub with enhanced productivity and readability",
  "author": "Hacktoberfest Contributor",
  "theme": {
    "colors": {
      "primary": "#0969da",
      "secondary": "#656d76",
      "background": "#ffffff",
      "background-dark": "#0d1117",
      "background-secondary": "#f6f8fa",
      "background-secondary-dark": "#161b22",
      "text": "#24292f",
      "text-dark": "#f0f6fc",
      "text-secondary": "#656d76",
      "text-secondary-dark": "#8b949e",
      "border": "#d1d9e0",
      "border-dark": "#30363d",
      "accent": "#1f883d",
      "warning": "#9a6700",
      "error": "#cf222e",
      "success": "#1a7f37",
      "info": "#0969da",
      "highlight": "#fff8c5",
      "highlight-dark": "#6e7681"
    },
    "typography": {
      "font-family": "-apple-system, BlinkMacSystemFont, 'Segoe UI', 'Noto Sans', Helvetica, Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji'",
      "font-size-base": "14px",
      "line-height": "1.5",
      "font-weight-normal": "400",
      "font-weight-medium": "500",
      "font-weight-semibold": "600",
      "font-weight-bold": "700"
    },
    "spacing": {
      "small": "8px",
      "medium": "16px",
      "large": "24px"
    },
    "border-radius": "6px",
    "shadows": {
      "small": "0 1px 0 rgba(27, 31, 36, 0.04)",
      "medium": "0 3px 6px rgba(140, 149, 159, 0.15)",
      "large": "0 8px 24px rgba(140, 149, 159, 0.2)",
      "inset": "inset 0 1px 0 rgba(255, 255, 255, 0.25)"
    }
  },
  "features": [
    "Clean and modern design",
    "Enhanced readability and contrast",
    "Professional color palette",
    "Optimized typography",
    "Subtle shadows and borders",
    "Dark mode support",
    "Accessibility focused",
    "Productivity enhancements",
    "Consistent spacing",
    "Minimal distractions"
  ],
  "compatibility": {
    "github": ">=2023.1.0",
    "browsers": ["Chrome", "Firefox", "Safari", "Edge"]
  },
  "professional_features": {
    "focus_mode": "Minimal UI for deep work",
    "accessibility": "WCAG 2.1 AA compliant",
    "productivity": "Enhanced code readability",
    "consistency": "Unified design language",
    "performance": "Optimized for speed"
  }
}
